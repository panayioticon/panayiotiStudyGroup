@model IEnumerable<panayiotiStudyGroup.Models.GroupMember>
@{
    ViewBag.Title = "ListOfMembers";
}

<h2>List Of Members</h2>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.StudentNumber) </th>
        <th>@Html.DisplayNameFor(model => model.Name) </th>
        <th>@Html.DisplayNameFor(model => model.Surname) </th>
        <th>@Html.DisplayNameFor(model => model.EmailAddress) </th>
    </tr>



    @foreach (var i in Model)
    {
        <tr>
            <td>@Html.DisplayFor(item => i.StudentNumber) </td>
            <td>@Html.DisplayFor(item => i.Name) </td>
            <td>@Html.DisplayFor(item => i.Surname) </td>
            <td>@Html.DisplayFor(item => i.EmailAddress) </td>
        </tr>

    }
</table>